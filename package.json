{
  "name": "team-calendar",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:e2e": "bunx playwright test",
    "test:vrt": "bunx playwright test visual-regression.e2e.ts --workers=4",
