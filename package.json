    "tsc": "tsc",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "check": "biome check ."
  },
  "dependencies": {
    "astro": "^5.11.0",
    "nanostores": "^0.11.4",
    "@nanostores/persistent": "^0.10.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@playwright/test": "^1.53.2",
    "@types/bun": "latest",
    "lefthook": "^1.11.16",
    "typescript": "^5.8.3"
  }
